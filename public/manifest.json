{
  "manifest_version": 3,
  "name": "NHN Forticlient Close Extensions",
  "description": "NHN Forticlient Close Extensions",
  "version": "0.1",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icons/icon-96x96.png"
  },
  "icons": {
    "16": "icons/icon-72x72.png",
    "48": "icons/icon-96x96.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["scripting", "tabGroups", "activeTab", "tabs"],
  "host_permissions": ["http://*/*"],
  "optional_host_permissions": ["*://*/*"]
}
